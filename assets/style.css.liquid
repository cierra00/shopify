/* FLAG DARKEN */
{% assign color_shift = settings.color_4 %}
{% assign color_shift_value = -20 %}

{% assign node1 = color_shift | slice: 1 %}
{% assign node2 = color_shift | slice: 2 %}
{% assign node3 = color_shift | slice: 3 %}
{% assign node4 = color_shift | slice: 4 %}
{% assign node5 = color_shift | slice: 5 %}
{% assign node6 = color_shift | slice: 6 %}

{% capture red1 %}{% case node1 %}{% when 'a' %}10{% when 'b' %}11{% when 'c' %}12{% when 'd' %}13{% when 'e' %}14{% when 'f' %}15{% else %}{{ node1 }}{% endcase %}{% endcapture %}
{% capture red2 %}{% case node2 %}{% when 'a' %}10{% when 'b' %}11{% when 'c' %}12{% when 'd' %}13{% when 'e' %}14{% when 'f' %}15{% else %}{{ node2 }}{% endcase %}{% endcapture %}
{% capture green1 %}{% case node3 %}{% when 'a' %}10{% when 'b' %}11{% when 'c' %}12{% when 'd' %}13{% when 'e' %}14{% when 'f' %}15{% else %}{{ node3 }}{% endcase %}{% endcapture %}
{% capture green2 %}{% case node4 %}{% when 'a' %}10{% when 'b' %}11{% when 'c' %}12{% when 'd' %}13{% when 'e' %}14{% when 'f' %}15{% else %}{{ node4 }}{% endcase %}{% endcapture %}
{% capture blue1 %}{% case node5 %}{% when 'a' %}10{% when 'b' %}11{% when 'c' %}12{% when 'd' %}13{% when 'e' %}14{% when 'f' %}15{% else %}{{ node5 }}{% endcase %}{% endcapture %}
{% capture blue2 %}{% case node6 %}{% when 'a' %}10{% when 'b' %}11{% when 'c' %}12{% when 'd' %}13{% when 'e' %}14{% when 'f' %}15{% else %}{{ node6 }}{% endcase %}{% endcapture %}

{% assign red = red1 | times:16 | plus:red2 | plus:color_shift_value | remove:'-' %}
{% assign green = green1 | times:16 | plus:green2 | plus:color_shift_value | remove:'-' %}
{% assign blue = blue1 | times:16 | plus:blue2 | plus:color_shift_value | remove:'-' %}

{% assign node1 = red | divided_by:16 %}
{% assign node2 = red | modulo:16 %}
{% assign node3 = green | divided_by:16 %}
{% assign node4 = green | modulo:16 %}
{% assign node5 = blue | divided_by:16 %}
{% assign node6 = blue | modulo:16 %}

{% capture red1 %}{% case node1 %}{% when 10 %}a{% when 11 %}b{% when 12 %}c{% when 13 %}d{% when 14 %}e{% when 15 %}f{% else %}{{ node1 }}{% endcase %}{% endcapture %}
{% capture red2 %}{% case node2 %}{% when 10 %}a{% when 11 %}b{% when 12 %}c{% when 13 %}d{% when 14 %}e{% when 15 %}f{% else %}{{ node2 }}{% endcase %}{% endcapture %}
{% capture green1 %}{% case node3 %}{% when 10 %}a{% when 11 %}b{% when 12 %}c{% when 13 %}d{% when 14 %}e{% when 15 %}f{% else %}{{ node3 }}{% endcase %}{% endcapture %}
{% capture green2 %}{% case node4 %}{% when 10 %}a{% when 11 %}b{% when 12 %}c{% when 13 %}d{% when 14 %}e{% when 15 %}f{% else %}{{ node4 }}{% endcase %}{% endcapture %}
{% capture blue1 %}{% case node5 %}{% when 10 %}a{% when 11 %}b{% when 12 %}c{% when 13 %}d{% when 14 %}e{% when 15 %}f{% else %}{{ node5 }}{% endcase %}{% endcapture %}
{% capture blue2 %}{% case node6 %}{% when 10 %}a{% when 11 %}b{% when 12 %}c{% when 13 %}d{% when 14 %}e{% when 15 %}f{% else %}{{ node6 }}{% endcase %}{% endcapture %}

{% capture flag_darken_color %}#{{ red1 }}{{ red2 }}{{ green1 }}{{ green2 }}{{ blue1 }}{{ blue2 }}{% endcapture %}



